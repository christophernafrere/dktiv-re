
generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model User {
  id        String   @id @default(uuid())
  firstName      String
  lastName  String
  pseudo  String
  email     String   @unique
  password String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}